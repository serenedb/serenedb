{
    "name": "serene-ui-web",
    "version": "0.1.0",
    "description": "React App for SereneUI web version",
    "type": "module",
    "scripts": {
        "dev:docker": "vite --mode dev-docker",
        "build:docker": "tsc -b && vite build --mode prod-docker",
        "dev:electron": "vite --mode dev-electron",
        "build:electron": "tsc -b && vite build --mode prod-electron",
        "dev": "npm run dev:docker",
        "build": "npm run build:electron && npm run build:docker",
        "storybook": "storybook dev -p 6006",
        "test-storybook": "vitest run --config=.storybook/vitest.workspace.ts --mode test-docker && node .storybook/vitest.teardown.ts",
        "test-storybook:renew": "UPDATE_SNAPSHOTS=true npm run test-storybook",
        "test-storybook:clean": "node .storybook/vitest.teardown.ts"
    },
    "dependencies": {
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-context-menu": "^2.2.16",
        "@radix-ui/react-dialog": "^1.1.15",
