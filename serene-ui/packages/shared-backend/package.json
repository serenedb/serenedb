        "winston": "^3.15.0"
    },
    "devDependencies": {
        "@types/better-sqlite3": "^7.6.13",
        "@types/express": "^4.17.21",
        "@types/node": "^20.0.0",
        "@types/pg": "^8.15.5",
        "@types/pg-cursor": "^2.7.2",
        "rimraf": "^5.0.0",
