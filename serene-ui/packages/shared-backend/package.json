{
    "name": "@serene-ui/shared-backend",
    "version": "0.1.0",
    "description": "Shared backend entities for Node.js/Express/Electron",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
