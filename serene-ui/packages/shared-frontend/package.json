{
    "name": "@serene-ui/shared-frontend",
    "version": "0.1.0",
    "description": "SereneUI shared widgets, features and entities",
    "type": "module",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -b --force && node scripts/copy-assets.mjs",
        "dev": "tsc --watch"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./features": {
            "types": "./dist/features/index.d.ts",
            "default": "./dist/features/index.js"
        },
        "./widgets": {
            "types": "./dist/widgets/index.d.ts",
            "default": "./dist/widgets/index.js"
        },
        "./shared": {
            "types": "./dist/shared/index.d.ts",
            "default": "./dist/shared/index.js"
        },
        "./entities": {
            "types": "./dist/entities/index.d.ts",
            "default": "./dist/entities/index.js"
        }
    },
    "dependencies": {
        "@orpc/tanstack-query": "^1.13.1",
        "@serene-ui/shared-core": "*"
    },
    "peerDependencies": {
        "@radix-ui/react-checkbox": "^1.3.3",
        "@radix-ui/react-context-menu": "^2.2.16",
        "@radix-ui/react-dialog": "^1.1.15",
        "@radix-ui/react-dropdown-menu": "^2.1.16",
        "@radix-ui/react-hover-card": "^1.1.15",
        "@radix-ui/react-label": "^2.1.7",
        "@radix-ui/react-popover": "^1.1.15",
        "@radix-ui/react-select": "^2.2.6",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-tooltip": "^1.2.8",
        "@tanstack/react-query": "^5.87.4",
        "@tanstack/react-table": "^8.21.3",
        "@tanstack/react-virtual": "^3.13.12",
        "cmdk": "^1.1.1",
        "input-otp": "^1.4.2",
        "lucide-react": "^0.544.0",
        "monaco-editor": "^0.53.0",
        "next-themes": "^0.4.6",
        "react": "^19.1.1",
        "react-arborist": "^3.4.3",
        "react-dom": "^19.1.1",
        "react-hotkeys-hook": "^5.1.0",
        "react-i18next": "^15.7.3",
        "react-resizable-panels": "^3.0.6",
        "react-router-dom": "^7.8.2",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@types/react": "^19.1.13",
        "@types/react-dom": "^19.1.9",
        "class-variance-authority": "^0.7.1",
        "rimraf": "^5.0.0",
        "typescript": "^5.6.3",
        "vite": "^7.1.2"
    },
    "license": "Apache-2.0"
}
