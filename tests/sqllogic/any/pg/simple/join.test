query
SELECT
    tindex.*,
    clickclack.*,
    idb.*,
    iq, tindex.iq, clickclack.iq, idb.iq,
    name, tindex.name, clickclack.name, idb.name,
    tindex.grade, tindex.title,
    clickclack.weight, clickclack.job_title,
    idb.age, idb.curse
FROM
(
    VALUES (228, 54, 'vedernikoff', 'intern'), (54, 154, 'babsky', 'boss')
) AS tindex(iq, grade, name, title)
JOIN
(
    (
        VALUES (228, 54, 'vedernikoff', 'crook'), (54, 44, 'babsky', 'mafia boss')
    ) AS idb(iq, age, name, curse)
