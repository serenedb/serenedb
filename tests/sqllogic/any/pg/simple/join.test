query
SELECT *
FROM
(
    VALUES (228, 54, 'vedernikoff', 'intern'), (54, 154, 'babsky', 'boss')
) AS tindex(iq, grade, name, title)
JOIN
(
    VALUES (228, 154, 'vedernikoff', 'boss'), (54, 44, 'babsky', 'cleaner')
) AS clickclack(iq, weight, name, job_title)
USING (name);
----
name	iq	grade	title	iq	weight	job_title
vedernikoff	228	54	intern	228	154	boss
babsky	54	154	boss	54	44	cleaner


