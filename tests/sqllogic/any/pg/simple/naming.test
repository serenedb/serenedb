query
SELECT
    t1.frog_name,
    t1.power_level,
    t1.secret_data,
    outfit_type,
    t2.creator_alias,
    t3.magic_skill,
    energy_power
FROM
(
  SELECT
      inner_frogs.pepe_name as frog_name,
      inner_power.internal_level as power_level,
      inner_frogs.hidden_col as secret_data
  FROM
      (
          VALUES ('Konstantin', 'secret1'), ('vedernikoff', 'secret2'), ('pavel', 'secret3')
      ) AS inner_frogs(pepe_name, hidden_col),
      (
          VALUES (8500), (9200), (7800), (9500)
      ) AS inner_power(internal_level)
  WHERE inner_frogs.pepe_name != 'pavel'
) AS t1(frog_name, power_level),
(
  SELECT
      costume_data.style_val as outfit_type,
      creator_data.name_val as creator_alias,
      costume_data.extra_info
  FROM
      (
          VALUES ('ivanoff', 'extra1'), ('serenedb', 'extra2'), ('arelav', 'extra3')
      ) AS costume_data(style_val, extra_info),
      (
          VALUES ('androed'), ('abramov'), ('Konstantin'), ('vedernikoff')
      ) AS creator_data(name_val)
  WHERE creator_data.name_val = 'Konstantin'
) AS t2(outfit_type, creator_alias, unused_col),
(
  SELECT
      magic_data.skill_val as magic_skill,
      magic_data.power_val as energy_power,
      magic_data.hidden_val
  FROM
      (
          VALUES ('vedernikoff', 150, 'hide1'), ('pavel', 250, 'hide2'), ('ivanoff', 350, 'hide3')
      ) AS magic_data(skill_val, power_val, hidden_val),
      (
          VALUES (100), (200), (300)
      ) AS multiplier(mult_val)
  WHERE magic_data.power_val != 350
) AS t3
WHERE t1.power_level > 8000
  AND t3.energy_power < 300
  AND t2.outfit_type != 'arelav'
  AND secret_data = 'secret1'
  AND outfit_type = 'serenedb'
  AND t1.power_level > 180
ORDER BY
  frog_name,
  power_level,
  secret_data,
  outfit_type,
  creator_alias,
  magic_skill,
  energy_power;
